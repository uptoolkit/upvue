{
  "name": "upvue",
  "description": "Vue toolkit of plugins to bootstrap your VueApp with everything you need",
  "homepage": "https://github.com/uptoolkit/upvue",
  "version": "1.2.24",
  "author": "Daniel Sum",
  "email": "daniel@cherrypulp.com",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && rm -R ./dist/api && rm -R ./dist/img && rm -R ./dist/favicon.ico && yarn run types",
    "types": "node build-types.js",
    "serve": "vite preview",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "main": "./dist/upvue.umd.js",
  "module": "./dist/upvue.es.js",
  "exports": {
    ".": {
      "import": "./dist/upvue.es.js",
      "require": "./dist/upvue.umd.js"
    }
  },
  "typings": "types/index.d.ts",
  "keywords": [
    "vue",
    "library",
    "playwork",
    "boilerplate",
    "starter"
  ],
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@bundled-es-modules/axios": "^0.21.1",
    "@cherrypulp/i18n": "^1.4.2",
    "@headlessui/vue": "^1.4.0",
    "@heroicons/vue": "^1.0.4",
    "@rollup/plugin-dynamic-import-vars": "^1.4.0",
    "@tailwindcss/forms": "^0.3.4",
    "@types/node": "^16.11.4",
    "@vue/apollo-composable": "^4.0.0-alpha.15",
    "@vue/apollo-option": "^4.0.0-alpha.15",
    "@vueuse/core": "^6.5.3",
    "@vueuse/integrations": "^6.5.3",
    "@vueuse/router": "^6.5.3",
    "ant-design-vue": "^2.2.3",
    "autoprefixer": "^10.3.7",
    "axios": "^0.23.0",
    "collect.js": "^4.29.0",
    "core-js": "^3.16.1",
    "daisyui": "^1.14.2",
    "form-backend-validation": "^2.4.0",
    "graphql": "^15.6.1",
    "graphql-tag": "^2.12.5",
    "js-config-helper": "^1.1.0",
    "js-form-helper": "^1.2.2",
    "lodash": "^4.17.21",
    "tailwindcss": "^2.2.16",
    "vue": "^3.1.5",
    "vue-router": "4",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@originjs/vite-plugin-commonjs": "^1.0.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/compiler-sfc": "^3.0.4",
    "babel-loader": "^8.2.2",
    "lerna": "^4.0.0",
    "npm-dts": "^1.3.10",
    "ts-morph": "^12.0.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vue-loader": "^16.8.1",
    "vue-tsc": "^0.3.0"
  }
}
